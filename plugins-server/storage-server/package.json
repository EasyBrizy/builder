    "@types/multer": "^1.4.11"
  },
  "dependencies": {
    "@brizy/core-server": "latest",
    "dotenv": "^16.4.5",
    "express-fileupload": "^1.4.3",
    "multer": "^1.4.5-lts.1"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "npm": ">=6.0.0"
