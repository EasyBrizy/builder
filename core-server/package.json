{
  "name": "@brizy/core-server",
  "version": "1.0.1",
  "description": "Brizy Core-server for pluggable apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "brizy",
    "core-server"
  ],
  "author": "brizy.io",
  "license": "MIT",
  "homepage": "https://github.com/EasyBrizy/builder#readme",
  "bugs": "https://github.com/EasyBrizy/builder/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyBrizy/builder.git"
  },
  "scripts": {
    "lint": "eslint \"src/**/!(*.d).ts*\"",
    "prettier-check": "prettier --check \"src/**.{ts,tsx}\"",
    "clean": "rimraf dist",
    "clean:modules": "find . -name 'node_modules' -type d -prune -print -exec rm -rf '{}' \\;",
    "reinstall": "npm run clean:modules && npm install",
    "start": "node dist/index.js",
    "build": "npm run clean && tsc --emitDeclarationOnly && swc src -d dist --strip-leading-paths"
  },
  "devDependencies": {
    "@brizy/eslint-config": "*",
    "@brizy/prettier-config": "*",
    "@brizy/tsconfig": "*",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.11"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.3"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "npm": ">=6.0.0"
  }
}
