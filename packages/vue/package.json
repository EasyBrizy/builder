{
  "name": "@brizy/vue",
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,vue}\"",
    "prettier-check": "prettier --check \"src/**/*.{ts,vue}\"",
    "tsc": "vue-tsc --noEmit",
    "build": "npm run clean && vue-tsc && vite build --minify=false",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "@brizy/assetmanager": "*"
  },
  "devDependencies": {
    "@brizy/prettier-config": "*",
    "@brizy/eslint-config": "*",
    "@brizy/tsconfig": "*",
    "@brizy/sdk": "*",
    "rimraf": "^5.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "path": "^0.12.7",
    "postcss": "^8.4.25",
    "rollup-plugin-typescript2": "^0.34.1",
    "tailwindcss": "^3.3.2",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vue-tsc": "^1.0.11"
  }
}
