{
  "name": "@brizy/vue",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint \"src/**.{ts,vue}\"",
    "prettier-check": "prettier --check \"src/**.{ts,vue}\"",
    "tsc": "vue-tsc --noEmit",
    "dev": "npm run clean && npm run build:types && webpack --watch",
    "build": "npm run clean && npm run build:types && webpack --mode production",
    "build:types": "vue-tsc --emitDeclarationOnly",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "@brizy/assetmanager": "*"
  },
  "devDependencies": {
    "@brizy/prettier-config": "*",
    "@brizy/eslint-config": "*",
    "@brizy/tsconfig": "*",
    "@swc/core": "^1.3.67",
    "path-browserify": "^1.0.1",
    "rimraf": "^5.0.1",
    "swc-loader": "^0.2.3",
    "util": "^0.12.5",
    "vue-loader": "^17.2.2",
    "vue-template-compiler": "^2.7.14",
    "vue-tsc": "^1.8.3",
    "webpack": "^5.85.1"
  },
  "files": [
    "package.json",
    "dist"
  ]
}
