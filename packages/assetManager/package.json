{
  "name": "@brizy/assetmanager",
  "version": "1.0.0",
  "description": "Brizy Asset Manager, used when need to organize the output of assets from Compiler",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "npm run clean && npm run build:types && node bin/index.js",
    "build:prod": "npm run build -- --production",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "eslint --ext js,ts,tsx src",
    "clean": "rimraf dist",
    "prettier-check": "prettier --check \"src/**.{ts,tsx}\"",
    "tsc": "tsc --noEmit"
  },
  "devDependencies": {
    "@brizy/eslint-config": "*",
    "@brizy/tsconfig": "*",
    "@swc/core": "^1.3.66",
    "process": "^0.11.10",
    "rimraf": "^5.0.1",
    "swc-loader": "^0.2.3",
    "esbuild": "^0.18.6",
    "minimist": "^1.2.8",
    "@brizy/prettier-config": "*",
    "@brizy/utils": "*"
  },
  "author": "Brizy",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "fp-utilities": "^1.1.4"
  },
  "files": [
    "package.json",
    "dist"
  ]
}
