{
  "name": "@brizy/content-placeholder",
  "version": "1.0.5",
  "description": "Brizy content placeholders used to put dynamic data in compiled page (PREVIEW)",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "tsc --watch",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "npx swc ./src -d ./dist/cjs/",
    "lint": "eslint ./src",
    "tsc": "tsc --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest test",
    "check": "npm run lint && npm run tsc && npm run test",
    "clean": "rimraf ./dist",
    "prettier:check": "prettier --config .prettierrc --check src\"**/*.*\"",
    "prettier:write": "prettier --config .prettierrc --write src\"**/*.*\"",
    "prepublishOnly": "npm run check && npm run build"
  },
  "keywords": [
    "brizy",
    "content",
    "placeholder"
  ],
  "author": "brizy",
  "license": "ISC",
  "homepage": "https://brizy.io",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.85",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.6",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "5.1.6"
  },
  "files": [
    "dist"
  ]
}
