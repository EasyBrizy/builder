{
  "name": "brizy-preview",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:nextjs": "turbo build --scope='@brizy/nextjs'",
    "build:nuxtjs": "turbo build --scope='@brizy/nuxtjs'",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "tsc": "turbo run tsc",
    "prettier-check": "turbo run prettier-check",
    "check": "npm run prettier-check && npm run lint && npm run tsc"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "turbo": "^1.9.7"
  },
  "packageManager": "npm@9.5.1",
  "workspaces": [
    "examples/*",
    "packages/*",
    "providers/*"
  ]
}
