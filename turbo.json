{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "web#build": {
      "dependsOn": ["@brizy/assetmanager#build", "@brizy/docs-react#build"]
    },
    "web#dev": {
      "dependsOn": ["@brizy/assetmanager#build", "@brizy/docs-react#build"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
